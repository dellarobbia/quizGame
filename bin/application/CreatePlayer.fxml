<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="155.0" prefWidth="291.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CreatePlayerController">
   <children>
      <HBox layoutX="14.0" layoutY="14.0" prefHeight="100.0" prefWidth="261.0">
         <children>
            <VBox prefHeight="78.0" prefWidth="255.0" spacing="10.0">
               <children>
                  <Label alignment="TOP_LEFT" prefHeight="37.0" prefWidth="256.0" text="Welcome to Andrew's Final Project Quiz Game! Please enter your name below:" wrapText="true" />
                  <TextField fx:id="playerNameTextField" />
               </children>
               <padding>
                  <Insets top="2.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
      <Button fx:id="startQuizButton" layoutX="230.0" layoutY="114.0" mnemonicParsing="false" onAction="#startQuizButton_Pressed" text="Start!" />
   </children>
</AnchorPane>
